<h1>All tasks</h1>
<% @tasks.each do |task| %>

<div class="card-product-s">
    <div class="card-product-infos">
    <div class="task-line-one">
      <div class="task-label">
        <p>Task</p>
      </div>
      <p>
        <%= task.title %>
      </p>
    </div>
    <div class="task-line-one">
      <div class="task-label">
        <p>Status</p>
      </div>
      <p>
        <% if task.status == false %>
          Incomplete
        <% else %>
          Complete
        <% end %>
      </p>
    </div>
    <div class="task-line-one">
      <div class="task-label">
        <p>Urgency</p>
      </div>
      <p>
        <%= task.urgency %>
      </p>
    </div>
    <div class="task-line-one">
      <div class="task-label">
        <p>Renter</p>
      </div>
      <p>
        <%= task.unit_id %>
      </p>
      <% if task.status == false %>
    </div>
  </div>
  <div class="task-middle-s">
    <div class="card-title-top">
      <h2>Building - Unit</h2>
      <p><%= task.created_at %></p>
    </div>
    <div class="task-description-s">
      <p>Description
        Pellentesque habitant morbi tristique et netus.
      </p>
    </div>
  </div>
  <div class="right-side-card">
    <div class="edit-status">
      <button class="btn btn-success btn-sm" id="task-complete">
        <%= link_to "Mark task as complete", update_status_task_path(task), method: :patch %>
      </button>
      <a href="<%= edit_task_path(task) %>">
        <i class="fas fa-pencil-alt"></i>
      </a>
    </div>
    <div class="img-s">
        <img src="https://images-wixmp-ed30a86b8c4ca887773594c2.wixmp.com/f/61823bca-5449-4cb2-a81b-3380fbc18fbf/d52q49j-3103d1f6-a3bd-469f-8c0d-1c4a0fd7dd55.png?token=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9..RD65FbsMaDia38VSb9wxkT3JtU-MSJDcnruQlOhOFAw" />
        <img src="https://questden.org/kusaba/icons/src/wthamiseeingplz.gif" />
        <img src="https://images-wixmp-ed30a86b8c4ca887773594c2.wixmp.com/f/61823bca-5449-4cb2-a81b-3380fbc18fbf/d52q49j-3103d1f6-a3bd-469f-8c0d-1c4a0fd7dd55.png?token=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9..RD65FbsMaDia38VSb9wxkT3JtU-MSJDcnruQlOhOFAw" />
      <div>
        <img src="https://images-wixmp-ed30a86b8c4ca887773594c2.wixmp.com/f/61823bca-5449-4cb2-a81b-3380fbc18fbf/d52q49j-3103d1f6-a3bd-469f-8c0d-1c4a0fd7dd55.png?token=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9..RD65FbsMaDia38VSb9wxkT3JtU-MSJDcnruQlOhOFAw" />
        <img src="https://questden.org/kusaba/icons/src/wthamiseeingplz.gif" />
        <img src="https://images-wixmp-ed30a86b8c4ca887773594c2.wixmp.com/f/61823bca-5449-4cb2-a81b-3380fbc18fbf/d52q49j-3103d1f6-a3bd-469f-8c0d-1c4a0fd7dd55.png?token=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9..RD65FbsMaDia38VSb9wxkT3JtU-MSJDcnruQlOhOFAw" />
      </div>
    </div>
  </div>
      <% end %>
  <%# <img src="https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/skateboard.jpg" /> %>
</div>

<% end %>

